@model IEnumerable<StrategyModel>

@{
    var count = 1;
}
<br />
<h2>Standard Bigram Frequency Table</h2>
<br />

<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Character</th>
            <th scope="col">Frequency Percentage</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var strategyModel in Model)
        {
            @foreach (var mapping in strategyModel.FrequencyGramMapping)
            {
                <tr>
                    <td scope="row">@(count++)</td>
                    <td>@mapping.Key</td>
                    <td>@mapping.Value %</td>
                </tr>

            }

        }

    </tbody>
</table>
