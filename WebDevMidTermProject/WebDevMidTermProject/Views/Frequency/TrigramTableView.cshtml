@model IEnumerable<StrategyModel>

@{
    var count = 1;
}

<h2>Standard Trigram Frequency Table</h2>

<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Character</th>
            <th scope="col">Rank</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var strategyModel in Model)
        {
            @foreach (var mapping in strategyModel.FrequencyGramMapping)
            {
                <tr>
                    <td scope="row">@(count++)</td>
                    <td>@mapping.Key</td>
                    <td>@mapping.Value</td>
                </tr>
            }

        }

    </tbody>
</table>
